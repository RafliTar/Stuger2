<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.felix.stuger">

    <uses-feature android:name="android.hardware.camera" android:required="false"></uses-feature>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>.
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>.
    <uses-permission android:name="android.permission.MEDIA_CONTENT_CONTROL"></uses-permission>.
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    
    <application
        android:allowBackup="true"
        android:icon="@mipmap/stuger_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
             <intent-filter>
             <action android:name="android.intent.action.MAIN" />

             <category android:name="android.intent.category.LAUNCHER" />
             </intent-filter>
        </activity>
        <activity android:name=".ForumGeneral" />
        <activity
            android:name=".DashboardActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".PageQuestion" />
        <activity android:name=".SignUpStepOneActivity" />
        <activity android:name=".SignUpStepTwoActivity" />
        <activity android:name=".QuestionActivity">
        </activity>
    </application>

</manifest>